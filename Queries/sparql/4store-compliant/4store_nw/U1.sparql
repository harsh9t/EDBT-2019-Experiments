select ?name ?unitPrice where { {?a <http://northwind.com/model/productName> ?name. ?a <http://northwind.com/model/unitPrice> ?unitPrice . FILTER(?unitPrice <4.0 ). } UNION {?a <http://northwind.com/model/productName> ?name. ?a <http://northwind.com/model/unitPrice> ?unitPrice . FILTER(?unitPrice <7.0 && ?unitPrice > 4.0).} } 